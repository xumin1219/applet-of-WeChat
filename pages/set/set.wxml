<!--pages/set/set.wxml-->
<view class="page">
 <view class="page__bd">
  <view class="weui-cells weui-cells_after-title">
   <view class="weui-cell" bindtap="clearStorage">
    <view class="weui-cell__bd">清除缓存</view>
    <view class="weui-cell__ft weui-cell__ft_in-access">{{storage}}</view>
   </view>
   <view class="weui-cell"  bindtap="about">
    <view class="weui-cell__bd">关于我们</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
   </view>
   <view class="weui-cell" bindtap='kefu'>
    <view class="weui-cell__bd">意见反馈</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
   </view>
  </view>
 <button class="weui-btn" type="warn">退出账号</button>

 </view>
</view>